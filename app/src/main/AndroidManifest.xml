<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.nate.loveframe">


    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>


    <application
        android:name=".LoveFrameApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:label">
        <activity
            android:name=".ui.activity.test.TestMainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.test.ActionSheetActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.test.OverlayMenuActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.test.SweetDialogActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.test.PhotoSelectTestActivity"
            android:screenOrientation="portrait"></activity>
        <!-- 图片选择器-->
        <activity
            android:name="me.nereo.multi_image_selector.MultiImageSelectorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.test.ProgressBarTestActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.test.DesignTabLayoutActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.test.SegmentMenuActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.test.TagEditorActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.test.TargetGroupActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.activity.test.SeekBarActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>
